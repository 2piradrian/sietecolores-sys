generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id      String @id @unique
  name    String @default("Desconocido")
  type    String @default("Desconocido")
  size    String @default("Desconocido")
  weight  Float  @default(0)
}